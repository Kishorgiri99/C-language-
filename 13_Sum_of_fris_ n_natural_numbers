/******************************************************************************
Q. Sum of frist n natural numbers.
*******************************************************************************/

#include <stdio.h>

int sumOfNaturalNumber(int n)
{   int sum =0;
    if(n == 0)
    {
      return 0;
    }
    
    for(int i = 1; i<=n ; i++)
    {
      sum +=i;        
    }
    return sum;
}


int main()
{   
    int res = sumOfNaturalNumber(10);
    printf("%d",res);
    return 0;
}
